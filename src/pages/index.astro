---
import PublicLayout from '@/layouts/PublicLayout.astro';
import LandingPage from '@/landing/LandingPage';
const robots = ["index", "follow"];
const title = "Incadev - Instituto de Capacitación y Desarrollo Virtual";
const description = "Incadev es un instituto que ofrece servicios de capacitación y desarrollo virtual para estudiantes y profesionales.";
const keywords = ["Incadev", "Instituto de Capacitación y Desarrollo Virtual", "Capacitación", "Desarrollo Virtual", "Estudiantes", "Profesionales"];
const url = "https://instituto.cetivirgendelapuerta.com";
const imageOG = "/ISOLOGOTIPO_VERTICAL.svg";
const imagetwt = "/ISOLOGOTIPO_VERTICAL.svg";

---

<PublicLayout {robots} {title} {description} {keywords} {url} {imageOG} {imagetwt}>
	<LandingPage client:load/>
</PublicLayout>
